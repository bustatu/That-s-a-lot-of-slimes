            <string>///Step

//Animation speed
image_speed = 0

//Set cursor to default
image_index = 0

//If gamepad is connected, the cursor can be controlled with the right stick
if(gamepad_is_connected(0))
{
    //Move using the right stick
    nx = x + gamepad_axis_value(0, gp_axisrh) * move_speed * adelta_time
    ny = y + gamepad_axis_value(0, gp_axisrv) * move_speed * adelta_time
    if(actual_x&gt;=0&amp;&amp;actual_x&lt;=view_wview[0])
    {
        x = nx
    }
    else 
    {
        if(actual_x&lt;0)
        {
            x = view_xview[0] + view_wview[0]
        }
        else
        {
            x = view_xview[0]
        }
    }
    if(actual_y&gt;=0&amp;&amp;actual_y&lt;=view_hview[0])
    {
        y = ny
    }
    else 
    {
        if(actual_y&lt;0)
        {
            y = view_yview[0] + view_hview[0]
        }
        else
        {
            y = view_yview[0]
        }
    }
}
else {
    //Position
    x = mouse_x
    y = mouse_y
}

</string>
